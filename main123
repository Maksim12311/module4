graph = {'0': set(["1"]),
'1': set(['0']),
'2': set (['4']),
'3': set(["1", "2"]),
'4': set(['2'])}

def dfs(graph,start,visited = []):
	stack = [start]
	while stack:
		node = stack.pop()
		if node not in visited:
			visited.append(node)
			stack.extend(v for v in graph[node] if v not in visited)
	return visited

print(dfs(graph, '3'))